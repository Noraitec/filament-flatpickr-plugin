{
    "name": "noraitec/filament-flatpickr-plugin",
    "description": "Plugin de Filament que integra Flatpickr con soporte completo para su API y configuraci√≥n avanzada de calendario.",
    "type": "filament-plugin",
    "license": "GPL-3.0-or-later",
    "autoload": {
        "psr-4": {
            "Noraitec\\FilamentFlatpickrPlugin\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Dreamex",
            "email": "dsotelo@noraitec.com"
        }
    ],
    "require": {
        "php": "^8.3",
        "laravel/framework": "^12.0",
        "filament/filament": "^3.0"
    },
    "require-dev": {
    "pestphp/pest": "^3.7",
    "nunomaduro/larastan": "^3.0",
    "nunomaduro/collision": "^8.7"
  },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
